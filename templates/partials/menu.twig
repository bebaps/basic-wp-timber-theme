<nav>
    <ul>
        {% for item in items %}
            <li class="{{ item.classes|join(' ') }}">
                <a href="{{ item.link }}" target="{{ item.target }}">{{ item.title }}</a>

                {% include 'partials/menu.twig' with {items: item.children} %}
            </li>
        {% endfor %}
    </ul>
</nav>
